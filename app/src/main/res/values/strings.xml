	<string name="title_activity_intro">GliderLink</string>
	<string name="intro_text">
	  <![CDATA[
	    <p>GliderLink helps you and your friends soar as a team, sharing information about your location and the lift you\'ve found.</p>
	    ]]>
	</string>
	<string name="permissions_explanation">
	  <![CDATA[
	    <p>GliderLink requires these permissions:</p>
	    <p><b>Location</b> - Used to show your location on the map, and for sending your location to other pilots.</p>
	    <p><b>Photos, media, and files</b> - For storing downloaded maps and log files. The app does not access your photos, media, or files unrelated to the app.</p>
	  ]]>
	</string>
	<string name="visit_website_text">
